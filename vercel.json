{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/[client]/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>.*)\\.queztlearn\\.com"
        }
      ]
    }
  ]
}
